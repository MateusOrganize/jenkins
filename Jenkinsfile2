pipeline {
    agent any
    stages{
        stage ("Inicial"){
            steps{
                echo "Iniciando a pipeline"
            }
        }

        stage ("Build"){
            steps{
                echo "Realiando o build da apliação..."
            }
        }

        stage ("Deploy para PROD"){
            steps{
                echo "Deploy realizado com sucesso."
            }
        }
    }
}
